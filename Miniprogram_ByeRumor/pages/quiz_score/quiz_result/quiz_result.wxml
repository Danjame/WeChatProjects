<!--pages/quiz_score/quiz_result/quiz_result.wxml-->
<view class="score-order">
  <view class="order-wrap">
    <text wx:for="{{score}}" class="order-num {{item == -1? 'wrong':''}}" bindtap='toScoreHint' wx:key="index" data-index="{{index}}">{{index+1}}</text>

  </view>
      <view class="btns">
        <button class='wechat' style="height:200rpx;width:200rpx;background:none;padding:0" open-type="share">
      <view class='iconfont icon-WeChat share-icon'></view>
      <text class="share-txt">分享给微信好友</text>
    </button>

    <button class="moments" style="height:200rpx;width:200rpx;background:none;padding:0">
      <text class='iconfont icon-Moments share-icon' style="color:#666;font-size:60rpx;" bindtap="testIt"></text>
      <text class="share-txt">分享到朋友圈</text>
    </button>
    </view>
</view>
