/* components/analyze-cmp/analyze-cmp.wxss */
@import "./../../icons/icon.wxss";
.analyze-wrap{
  position: relative;
  width: 680rpx;
  height: 700rpx;
  padding: 34rpx 50rpx 0;
  box-sizing: border-box;
  margin: 30rpx auto;
  background: #33CB98;
  border-radius: 40rpx;
}
.tit{
  font-size: 54rpx;
  margin: 20rpx 0;
}
.true-or-false{
  margin: 54rpx 0 120rpx;
  line-height: 1;
  font-weight: 700;
  font-size: 60rpx;
  text-align: center;
}
.anl{
  color: #fff;
  font-size: 36rpx;
  font-weight: normal;
  line-height: 1.2;
}
.falseFlag{
  position: absolute;
  right: 20rpx;
  bottom: 20rpx;
  width: 134rpx;
  line-height: 134rpx;
  border-radius: 50%;
  background: #FD6830;
  color: #fff;
  font-size: 64rpx;
  text-align: center;
}